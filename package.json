    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "email-templates": "^8.0.2",
    "eslint": "^7.7.0",
    "googleapis": "^66.0.0",
    "http-status-codes": "^2.1.4",
    "ioredis": "^4.19.4",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "json-transformer-node": "^1.0.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mkdirp": "^1.0.4",
    "moleculer-repl": "^0.6.4",
    "nodemailer": "^6.4.17",
    "nodemailer-smtp-transport": "^2.7.4",
    "redlock": "^4.2.0",
    "slug": "^4.0.2",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "moleculer-web": "^0.9.0",
    "moleculer-db": "^0.8.4",
    "moleculer-db-adapter-mongo": "^0.4.7",
    "node-nats-streaming": "^0.2.6",
    "moleculer": "^0.14.10"
  },
  "engines": {
    "node": ">= 10.x.x"
  },
